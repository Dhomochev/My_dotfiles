"~/.vimrc
"https://qiita.com/marrontan619/items/541a1374e1ac672977e6
"ln -s (From)./My_dotfiles/.vimrc (To)~/
"
set number
set nobackup
set title
set ambiwidth=double
set mouse=a
set clipboard=unnamedplus  "+clipboard  is need to use!!!
set tabstop=4
set expandtab
set shiftwidth=4
set smartindent

set list
set listchars=tab:»-,trail:-,eol:↲,extends:»,precedes:«,nbsp:%
"set nrformats-=octal
"set hidden
set history=50
set virtualedit=block
"set whichwrap=b,s,[,],<,>
set whichwrap=b,s,h,l,<,>,[,],~
set backspace=indent,eol,start
"set wildmenu




